HANDLE_VP_TO_OPC(IROPC) \
HANDLE_VP_TO_SD(SDOPC) \
HANDLE_VP_FPCONSTRAINT(2, 3) \
HANDLE_VP_TO_CONSTRAINED_INTRIN(experimental_constrained_##IROP) \
